#Task0_partb
sudo apt install figlet
figlet -l Einstein
mkdir compare && cd compare && wget https://www.bioinformatics.babraham.ac.uk/training/Introduction%20to%20Unix/unix_intro_data.tar.gz 
gunzip unix_intro_data.tar.gz
tar xvf unix_intro_data.tar
cd seqmonk_genomes/'Saccharomyces cerevisiae'/EF4/ && grep rRNA Mito.dat
cp Mito.dat /home/dr_deepak_iyer/compare/
nano /home/dr_deepak_iyer/compare/Mito.dat
cd /home/dr_deepak_iyer/compare/FastQ_Data
wc -l <lane8_DD_P4_TTAGGC_L008_R1.fastq.gz
wc -l *.fastq.gz > linecount.txt
